{
  "github_url": "https://github.com/Traia-IO/coinmarketcap-pro-mcp-server",
  "mcp_server": {
    "name": "coinmarketcap-pro-mcp",
    "description": "Cryptocurrency market data, rankings, and metrics from coinmarketcap pro api",
    "server_type": "streamable-http",
    "requires_api_key": true,
    "api_key_header": "Authorization",
    "api_keys": [
      "COINMARKETCAP_PRO_API_KEY"
    ],
    "capabilities": [
      "airdrops",
      "categories",
      "coinmarketcap_id_map_wqdt",
      "key_info",
      "postman_conversion_v1",
      "cmc_crypto_fear_and_greed_historical",
      "cmc_crypto_fear_and_greed_latest",
      "trending_gainers_losers",
      "trending_latest",
      "trending_most_visited",
      "quotes_historical_tn3t",
      "quotes_latest_unz1",
      "fcas_listings_latest_deprecated",
      "price_performance_stats_v1_deprecated",
      "quotes_historical_v1_deprecated",
      "quotes_latest_v1_deprecated",
      "quotes_historical",
      "ohlcv_historical_v2",
      "ohlcv_latest_v2",
      "price_performance_stats_v2",
      "quotes_historical_v2",
      "quotes_latest_v2",
      "quotes_historical_v3",
      "coinmarketcap_id_map",
      "coinmarketcap_id_map_cxma",
      "listings_latest",
      "listings_new",
      "ohlcv_historical_v1_deprecated",
      "ohlcv_latest_v1_deprecated"
    ],
    "payment_protocol": "402",
    "dual_mode": true,
    "dual_mode_description": "Supports both authenticated (free with API key) and payment (402 protocol) modes"
  },
  "deployment_method": "cloud_run",
  "gcp_project_id": "traia-mcp-servers",
  "gcp_region": "us-central1",
  "tags": [
    "coinmarketcap pro",
    "api",
    "mcp"
  ],
  "ref": "main"
}